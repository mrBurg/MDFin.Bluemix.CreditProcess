{
  "name": "mdfin_react",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "npm:init": "yarn",
    "npm:upgrade": "yarn upgrade --latest & npx npm-check-updates -u",
    "sql:init": "dotenv cross-var ts-node ./compilerScripts/prepareSql.ts",
    "next:build": "next build",
    "next:lint": "next lint",
    "rebuild": "npm rebuild node-sass",
    "lint": "eslint ./src/** ./pages/** ./compilerScripts/** -c .eslintrc.json --ext js,ts,jsx",
    "lint:fix": "eslint ./src/** ./pages/** ./compilerScripts/** -c .eslintrc.json --fix --ext js,ts,jsx",
    ":install": "dotenv -- cross-var yarn --registry=%COMPONENTS_REGISTRY% add",
    ":server": "node server/index.js",
    ":server:watch": "nodemon server/index.js",
    ":prepare": "ts-node ./compilerScripts/prepare.ts",
    ":libs": "ts-node ./compilerScripts/prepareLibs.ts",
    ":prebuild": "yarn :prepare -t & yarn :libs & yarn next:build",
    ":dev": "yarn :prepare & yarn :server",
    ":dev:watch": "yarn :prepare & yarn :server:watch",
    ":test": "yarn :prepare -t & yarn :libs & yarn next:build & yarn :server -t",
    ":prod": "yarn :prepare -p & yarn :libs & yarn next:build & yarn :server -p",
    "echo": "dotenv cross-var echo %COMPONENTS_REGISTRY%",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "eslintConfig": {
    "extends": "./.eslintrc.json"
  },
  "dependencies": {
    "@fingerprintjs/fingerprintjs": "^3.4.1",
    "@next/bundle-analyzer": "^14.0.4",
    "@types/react-helmet": "^6.1.6",
    "axios": "<1",
    "classnames": "^2.3.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "current-device": "^0.10.2",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "express-winston": "^4.2.0",
    "html-react-parser": "^4.0.0",
    "lodash": "^4.17.21",
    "md5": "^2.3.0",
    "mobx": "^6.9.0",
    "mobx-react": "^7.6.0",
    "moment": "^2.29.4",
    "next": "<13",
    "nextjs-sitemap-generator": "^1.3.1",
    "node-gettext": "^3.0.0",
    "nodeify-ts": "^1.0.6",
    "normalize.css": "^8.0.1",
    "rc-slider": "^10.2.1",
    "react": "^18.2.0",
    "react-accessible-accordion": "^5.0.0",
    "react-cookie": "^4.1.1",
    "react-datepicker": "^4.14.1",
    "react-device-detect": "^2.2.3",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-input-mask": "^2.0.4",
    "react-select": "^5.7.3",
    "react-slick": "^0.29.0",
    "react-snowfall": "^1.2.1",
    "semantic-ui-css": "^2.5.0",
    "semantic-ui-react": "^2.1.4",
    "slick-carousel": "^1.8.1",
    "styled-jsx": "^5.1.2",
    "winston": "^3.9.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.22.5",
    "@next/eslint-plugin-next": "<13",
    "@svgr/webpack": "^8.0.1",
    "@types/express": "^4.17.17",
    "@types/fingerprintjs2": "^2.0.0",
    "@types/fs-extra": "^11.0.1",
    "@types/lodash": "^4.14.195",
    "@types/md5": "^2.3.2",
    "@types/node": "<17",
    "@types/node-gettext": "^3.0.3",
    "@types/react": "^18.2.13",
    "@types/react-datepicker": "^4.11.2",
    "@types/react-dom": "^18.2.6",
    "@types/react-input-mask": "^3.0.2",
    "@types/react-slick": "^0.23.10",
    "@types/rimraf": "<4",
    "@types/yargs": "^17.0.24",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@typescript-eslint/parser": "^5.60.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "cross-var": "^1.1.0",
    "dotenv": "^16.3.1",
    "dotenv-cli": "^7.2.1",
    "eslint": "8.43.0",
    "eslint-config-next": "<13",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "fs-extra": "^11.1.1",
    "json-loader": "^0.5.7",
    "node-sass": "<8",
    "nodemon": "^3.0.1",
    "po-gettext-loader": "^1.0.0",
    "prettier": "^2.8.8",
    "sass": "^1.63.4",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3",
    "yargs": "^17.7.2"
  },
  "main": "index.js",
  "license": "MIT"
}
